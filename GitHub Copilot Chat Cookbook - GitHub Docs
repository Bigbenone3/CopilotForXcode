input.FormControl-radio[type="radio"]:checked:disabled, input.FormControl-radio[type="radio"][disabled]:checked { border-color: var(--control-fgColor-disabled,var(--color-primer-fg-disabled)); cursor: not-allowed; }

input.FormControl-radio[type="radio"]:checked:disabled ~ .FormControl-radio-labelWrap .FormControl-label, input.FormControl-radio[type="radio"][disabled]:checked ~ .FormControl-radio-labelWrap .FormControl-label { color: var(--control-fgColor-disabled,var(--color-primer-fg-disabled)); cursor: not-allowed; }

